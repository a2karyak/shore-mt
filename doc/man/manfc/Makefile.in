install-data-local: $(PDF_TARGETS) $(DOC_PDF_TARGETS)
	mkdir -p $(localpdfdir)
	cp $(DOC_PDF_TARGETS) $(PDF_TARGETS) $(localpdfdir)

uninstall-local: $(PDF_TARGETS) $(DOC_PDF_TARGETS)
	rm -rf $(localdocdir)

SUFFIXES: .fc 
