function execute  {
    print -n "$1\t--> running --> "
    print "running $1" >> $2
    if ./$1 >> $2 2>&1; then
	print "ok"
    else
	print "failed - $?"
    fi
}

rm -f /tmp/all*.out
outf=/tmp/all$$.out
execute bit $outf
execute "box 4 5,-908,300,44444/55555,-1,1,82"  $outf
execute "box 3 -908,300,44444/-1,1,82"  $outf
execute "box 2 -908,44444/-1,1"  $outf

execute cq1 $outf
execute cq2 $outf

execute errlogtest $outf
execute "fastnew fast 100000 200"  $outf
execute "fastnew regular 100000 200"  $outf

execute hash1 $outf
execute hash2 $outf

execute list1 $outf
execute list2 $outf
execute list3 $outf

execute heap $outf

execute ntime $outf

execute random $outf

execute rc $outf

execute stats $outf

echo "timer takes about 20 sec to run..."
execute timer $outf

execute vector $outf


print
print "result in $outf"
